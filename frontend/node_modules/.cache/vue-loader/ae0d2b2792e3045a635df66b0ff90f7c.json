.log(this.imageIndex);\n            console.log((index+this.imageIndex)%this.names.length);\n            console.log(this.names[(index+this.imageIndex)%this.names.length]);\n            return this.names[(index+this.imageIndex)%this.names.length];\n        },\n        getTitle(index){\n            return this.titles[(index+this.imageIndex)%this.titles.length];\n        },\n        scroll(dir){\n            this.imageIndex+=dir;\n            if (this.imageIndex<0){\n                this.imageIndex = this.images.length-1;\n            }\n        }\n    }        \n}\n</script>\n\n<style scoped>\n    #contributors{\n        padding-left:100px;\n        padding-right:100px;\n    }\n    .name{\n        font-family: Open Sans;\n        font-style: normal;\n        font-weight: bold;\n        font-size: 25px;\n        line-height: 34px;\n        text-align: center;\n        color: #000000;\n    }\n    .title{\n        font-family: Open Sans;\n        font-style: normal;\n        font-weight: normal;\n        font-size: 20px;\n        line-height: 27px;\n        text-align: center;\n        color: #858585;\n    }\n</style>"]}]}